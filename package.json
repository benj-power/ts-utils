{
  "name": "bp-ts-utils",
  "version": "1.8.0",
  "description": "Banjo's TypeScript Utilities",
  "author": "banjo OBE",
  "license": "Apache-2.0",
  "repository": "github:benj-power/ts-utils",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest --coverage --coverageReporters='text-summary' --watchAll=false",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "publish": "npm run build; "
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts}",
      "!src/mock/**/*.{ts}"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "jest": "^26.6.3",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "3.9.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "typescript",
    "javascript",
    "npm",
    "utilities"
  ],
  "dependencies": {
    "ts-node": "^9.0.0"
  }
}
